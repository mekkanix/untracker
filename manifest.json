{
  "name": "Untracker",
  "manifest_version": 3,
  "description": "An ad-tracking blocker that removes user-related tracking informations from popular websites.",
  "version": "1.0.0",
  "action": {
    "default_title": "Untracker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "./Untracker.js",
        "./utparser.js"
      ]
    }
  ]
}
